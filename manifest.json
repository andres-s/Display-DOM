{
  "name": "Display DOM",
  "version": "2.0",
  "description": "Recurse through a page's DOM tree and display it in a new tab.",
  "permissions": [
    "activeTab", "tabs"
  ],
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Display DOM"
  },
  
  "manifest_version": 2
}
